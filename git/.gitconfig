[core]
    pager = delta
    editor = vim
    quotepath = false
    excludesfile = ~/.gitignore_global
[interactive]
    diffFilter = delta --color-only
[delta]
    line-numbers = true
    navigate = true
    light = false
[pull]
    rebase = true
[merge]
    tool = vimdiff
    conflictstyle = diff3
[diff]
    colorMoved = default
[difftool]
    prompt=false
[color]
    interactive = true
    branch = true
    status = true
    diff = true
    ui = true
[commit]
    gpgSign = true
[gpg]
    program = gpg
[init]
    defaultBranch = main
